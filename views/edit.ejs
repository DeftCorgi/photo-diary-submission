
<form name="form"  enctype="multipart/form-data" method="UPDATE" action="/entry/edit/<%= entry.id %>">
    <div class="form">
        <div class="prop">
            <label>Change the image:</label>
            <div class="prop">
                <input type="file" name="photo">
            </div>
        </div>
        <div class="prop">
            <label>Title:</label>
            <div class="prop">
                <input type="text" name="title" placeholder=" <%= entry.description %> "></input>
            </div>
        </div>
        <div class="prop">
            <label>Description:</label>
            <div class="desc">
                <textarea name="description" placeholder="<%= entry.description %>"></textarea>
            </div>
        </div>
    </div>
    <div class="fButt"> 
        <input class="button" type="submit" value="Submit">
    </div>
    
</form>   

