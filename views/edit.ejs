
<form name="form"  method="POST" action="/entry/edit/<%= entry.id %>">
    <div class="form">
        <div class="prop">
            <label>Title:</label>
            <div class="prop">
                <input type="text" name="title" value=" <%= entry.title %> " />
            </div>
        </div>
        <div class="prop">
            <label>Description:</label>
            <div class="desc">
                <textarea name="description"><%= entry.description %></textarea>
            </div>
        </div>
    </div>
    <div class="fButt"> 
        <input class="button" type="submit" value="Submit">
    </div>
    
</form>   

