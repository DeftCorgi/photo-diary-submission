
<%for (var entry in entries) {%>
    <div class="photo"> 
        <a class="view" href="/entry/view/:id">
            <img src="<%= entries[entry].photoUrl %>"/> 
            <div class="overlay">
                <a href="#" class="delete"> X </a>
            </div>
            <div class="caption">
                <%= entries[entry].title %>
            </div>
        </a>
    </div>
<%}%>